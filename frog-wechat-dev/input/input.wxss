.frog-input {
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  border-bottom: 1px #E0E0E0 solid;
}

.frog-title {
  font-size: 28rpx;
  color: #495060;
  min-width: 150rpx;
}

.frog-input-style {
  width: 100%;
  font-size: 26rpx;
  color: #141414;
}

.frog-input-placeholder {
  font-size: 26rpx;
  color: #BBBBBB;
}